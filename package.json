{
  "name": "@ctdesarrollo/eslint-config",
  "version": "1.0.0",
  "description": "CT eslint configurations",
  "main": "base.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ct-desarrollo/eslint-config.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
